{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./build",
    "strict": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "baseUrl": "./",
    "paths": {
      "test/*": ["test/*"]
    }
  },
  "include": ["src/**/*", "utils", "utils", "utils", "public/test"],
  "exclude": ["src/**/*.test.ts"]
}
